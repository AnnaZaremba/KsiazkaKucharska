{% block formularzWyszukiwania %}
    <div>
        {{ form_start(form, { 'attr': { 'novalidate': 'novalidate'}}) }}
        <div id="formularz">
            <div>
                <div>
                    <label>Nazwa lub sk≈Çadnik</label>
                </div>
                <div>
                    {{ form_widget(form.nazwa, {'attr': {'class': 'form-control'}}) }}
                </div>
                <div id="blad">
                    {{ form_errors(form.nazwa) }}
                </div>
            </div>
            <div id="przycisk">
                <button class="btn btn-default" type="submit">Szukaj</button>
            </div>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}