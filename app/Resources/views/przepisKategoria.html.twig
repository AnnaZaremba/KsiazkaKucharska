{% block przepisKategoria %}
    {% block zdjecie %}
        {% include('::zdjecieKategoria.html.twig') %}
    {% endblock %}
    <div class="main">
        <div>
            <h2>{{ kategoria.nazwa|upper }}</h2>
            <div id="kategorie">
                <ul class="list-unstyled">
                    {% for przepis in przepisy %}
                        <h4><a href="{{ path('przepisid', { 'id': przepis.id }) }}">{{ przepis.nazwa }}</a></h4>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}